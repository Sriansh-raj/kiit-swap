            <input type="text" class="need" name="need" placeholder="Need (Section number only)" />
            <i class="fa fa-envelope"></i>
          </div>

          <div class="input-box">
            <input type="text" class="available" name="available" placeholder="Available (Section number only)" />
            <i class="fa fa-lock"></i>
          </div>
          <button type="submit">Update</button>
        </form>
      </div>
    </div>
