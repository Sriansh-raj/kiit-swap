    <% if (matchingUsers.length===0) { %>
      <p class="No-match"></p>
      <% } else { %>
        <div class="container1">
          <div id="matchingUsersContainer">
            <% matchingUsers.forEach(matchingUser=> { %>
              <h1 class="animatedText"></h1>
              <br /><br />
              <div class="matching-user">
                <h3>Name: <%= matchingUser.name %>
                </h3>
                <h3>Need: <%= matchingUser.need %>
                </h3>
                <h3>Available: <%= matchingUser.available %>
                </h3>
                <p>Phone Number: <%= matchingUser.number %>
